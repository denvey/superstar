}

.shadow-blur::before {
    background: inherit;
    border-radius: inherit;
    content: "";
    display: block;
    -webkit-filter: blur(5px);
    filter: blur(5px);
    height: 100%;
    left: 5px;
    opacity: .4;
    position: absolute;
    top: 5px;
    -webkit-transform: scale(1, 1);
    transform: scale(1, 1);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    width: 100%;
