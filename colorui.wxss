checkbox,
radio,
switch {
    position: relative
}

switch::after,
switch::before {
    color: #fff !important;
    content: "\e645";
    font-family: iconfont;
    font-size: 13px;
    left: 0px;
    line-height: 26px;
    pointer-events: none;
    position: absolute;
    text-align: center;
    top: 0;
    -webkit-transform: scale(0, 0);
    transform: scale(0, 0);
    transition: all .3s ease-in-out 0s;
    width: 26px;
    z-index: 9
}

switch::before {
    content: "\e646";
    left: auto;
    right: 0
}

checkbox::before,
radio::before,
switch::before,
switch[checked]::after {
    -webkit-transform: scale(1, 1);
    transform: scale(1, 1)
}

checkbox::before,
radio::before {
    color: #fff !important;
    content: "\e645";
    font-family: iconfont;
    font-size: 16px;
    line-height: 16px;
    margin-top: -8px;
    pointer-events: none;
    position: absolute;
    right: 5px;
    top: 50%;
    transition: all .3s ease-in-out 0s;
    z-index: 9
}


switch[checked]::before {
    -webkit-transform: scale(0, 0);
    transform: scale(0, 0)
}

switch .switch-input {
    border: none;
    border-radius: 50px;
    height: 26px;
    margin: 0;
    padding: 0 24px;
