}

.shadow-warp:after,
.shadow-warp:before {
    bottom: 15px;
    box-shadow: 0 15px 10px rgba(0, 0, 0, .2);
    content: "";
    left: 10px;
    position: absolute;
    top: 10px;
    -webkit-transform: rotate(-3deg);
    transform: rotate(-3deg);
    width: 50%;
    z-index: -1
}

