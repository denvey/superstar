checkbox,radio,switch {
    position: relative;
}

switch::after,switch::before {
    color: #fff!important;
    content: "\e645";
    font-family: iconfont;
    font-size: 26rpx;
    left: 0rpx;
    line-height: 26px;
    pointer-events: none;
    position: absolute;
    text-align: center;
    top: 0;
    transform: scale(0,0);
    transition: all .3s ease-in-out 0s;
    width: 26px;
    z-index: 9;
}

switch::before {
    content: "\e646";
    left: auto;
    right: 0;
}

checkbox::before,radio::before,switch::before,switch[checked]::after {
    transform: scale(1,1);
}

checkbox::before,radio::before {
    color: #fff!important;
    content: "\e645";
    font-family: iconfont;
    font-size: 32rpx;
    line-height: 16px;
    margin-top: -8px;
    pointer-events: none;
    position: absolute;
    right: 5px;
    top: 50%;
    transition: all .3s ease-in-out 0s;
    z-index: 9;
}

switch[checked]::before {
    transform: scale(0,0);
}

