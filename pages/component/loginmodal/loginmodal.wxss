.bg-mask {
    background-color: #333;
    position: relative;
}

.bg-mask::after {
    background-color: rgba(0,0,0,.5);
    border-radius: inherit;
    bottom: 0;
    content: "";
    display: block;
    height: 100%;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    width: 100%;
}

.bg-mask cover-view,.bg-mask view {
    position: relative;
    z-index: 5;
}

.bg-mask>cover-view {
    background-color: rgba(0,0,0,.5);
}

.bg-video {
    position: relative;
}

.bg-video video {
    display: block;
    height: 100%;
    object-fit: cover;
    pointer-events: none;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 0;
}

